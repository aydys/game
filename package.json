{
  "name": "game",
  "version": "1.0.0",
  "description": "Conway's Game of Life",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --mode development --open --hot",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0 --fix ./",
    "test": "npx jest",
    "test:loki": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "ts:check": "npx tsc -p tsconfig.json --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check": "npm run lint && npm run ts:check && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aydys/game.git"
  },
  "keywords": [
    "cellular",
    "automation"
  ],
  "author": "Aidys Dongak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aydys/game/issues"
  },
  "homepage": "https://github.com/aydys/game#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "loki": "^0.28.1",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "@emotion/styled": "^11.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
